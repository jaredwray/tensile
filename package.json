{
  "name": "tensile",
  "version": "0.1.1",
  "description": "Fastify Plugin for Modern API Development",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "test": "xo --fix && vitest run --coverage",
    "clean": "rimraf ./dist ./coverage ./node_modules package-lock.json",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepare": "tsup src/index.ts --format cjs,esm --dts --clean",
    "website:build": "docula",
    "website:serve": "docula serve"
  },
  "keywords": [],
  "author": "Jared Wray <me@jaredwray.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "docula": "^0.10.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5",
    "xo": "^0.60.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.2.1",
    "@fastify/swagger": "^9.4.0",
    "@scalar/fastify-api-reference": "^1.25.99",
    "cacheable": "^1.8.7",
    "fastify": "^5.2.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "read-package-up": "^11.0.0"
  }
}
